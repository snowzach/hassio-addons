{
  "name": "DOODS",
  "version": "2",
  "slug": "doods",
  "url": "https://github.com/snowzach/hassio-addons/tree/master/doods",
  "description": "Distributed Open Object Detection Service",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64"
  ],
  "startup": "services",
  "map": ["share"],
  "usb": true,
  "options": {
    "server": {
      "port": "8080"
    },
    "auth_key": "",
    "doods.detectors": [
      {
        "name": "default",
        "type": "tflite",
        "modelFile": "/opt/doods/models/coco_ssd_mobilenet_v1_1.0_quant.tflite",
        "labelFile": "/opt/doods/models/coco_labels0.txt",
        "numThreads": 1,
        "numConcurrent": 1,
        "hwAccel": false
      },
      {
        "name": "tensorflow",
        "type": "tensorflow",
        "modelFile": "/opt/doods/models/faster_rcnn_inception_v2_coco_2018_01_28.pb",
        "labelFile": "/opt/doods/models/coco_labels1.txt",
        "numThreads": 1,
        "numConcurrent": 1,
        "hwAccel": false
      }
    ]
  },
  "schema": {
    "server": {
      "port": "port?"
    },
    "doods.detectors": [
      {
        "name": "str",
        "type": "str",
        "modelFile": "str",
        "labelFile": "str?",
        "numThreads": "int?",
        "numConcurrent": "int?",
        "hwAccel": "bool?"
      }
    ],
    "auth_key": "str?"
  },
  "ports": {
    "8080/tcp": 8080
  },
  "ports_description": {
    "8080/tcp": "DOODS port"
  }
}
